{
  "Kafka": {
    "BootstrapServers": "pkc-xxxxx.confluent.cloud:9092",
    "SecurityProtocol": "SaslSsl",
    "SaslMechanism": "Plain",
    "SaslUsername": "<confluent_api_key>",
    "SaslPassword": "<confluent_api_secret>",
    "Producer": {
      "Acks": "all",
      "EnableIdempotence": true,
      "CompressionType": "snappy",
      "MessageTimeoutMs": 120000
    },
    "Consumer": {
      "GroupId": "http-to-kafka-default",
      "AutoOffsetReset": "earliest",
      "EnableAutoCommit": true,
      "SessionTimeoutMs": 45000,
      "MaxPollIntervalMs": 300000
    }
  },
  "SchemaRegistry": {
    "Url": "https://xxxxx.us-east-2.aws.confluent.cloud",
    "BasicAuthUserInfo": "<sr_api_key>:<sr_api_secret>",
    "BasicAuthCredentialsSource": "USER_INFO"
  },
  "Logging": { "LogLevel": { "Default": "Information", "Microsoft.AspNetCore": "Warning" } },
  "AllowedHosts": "*"
}
