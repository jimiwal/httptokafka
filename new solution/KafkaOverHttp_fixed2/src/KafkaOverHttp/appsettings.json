{
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "SecurityProtocol": "Plaintext",
    "SaslMechanism": "",
    "SaslUsername": "",
    "SaslPassword": "",
    "ClientId": "KafkaOverHttp",
    "GroupId": "KafkaOverHttp-Consumers",
    "AutoOffsetReset": "Earliest",
    "EnableAutoCommit": false,
    "SchemaRegistry": {
      "Url": "http://localhost:8081",
      "BasicAuthUserInfo": ""
    }
  },
  "Push": {
    "HttpTimeoutSeconds": 15,
    "MaxParallelDeliveries": 8,
    "RetryPolicy": {
      "RetryCount": 3,
      "BaseDelayMs": 500
    }
  },
  "AllowedHosts": "*"
}